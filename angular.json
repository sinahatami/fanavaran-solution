{
  "version": 1,
  "projects": {
    "Common": {
      "projectType": "application",
      "root": "apps/Common",
      "sourceRoot": "apps/Common/src",
      "prefix": "FanavaranSolution",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/Common",
            "index": "apps/Common/src/index.html",
            "main": "apps/Common/src/main.ts",
            "polyfills": "apps/Common/src/polyfills.ts",
            "tsConfig": "apps/Common/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": ["apps/Common/src/favicon.ico", "apps/Common/src/assets"],
            "styles": ["apps/Common/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/Common/src/environments/environment.ts",
                  "with": "apps/Common/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "Common:build:production"
            },
            "development": {
              "browserTarget": "Common:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "Common:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/Common/src/**/*.ts",
              "apps/Common/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/Common"],
          "options": {
            "jestConfig": "apps/Common/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "Common-e2e": {
      "root": "apps/Common-e2e",
      "sourceRoot": "apps/Common-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/Common-e2e/cypress.json",
            "devServerTarget": "Common:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "Common:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/Common-e2e/**/*.{js,ts}"]
          }
        }
      },
      "tags": [],
      "implicitDependencies": ["Common"]
    },
